    ,ApplyMap('ОплМес', %МесяцГодКонтрагентОп, 0) as НовыйВМесяце
    ,ApplyMap('ОплКврт', %КвртКонтрагентОп, 0) as НовыйВКвартале
RESIDENT ВрОпл;

