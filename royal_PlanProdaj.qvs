[ОплГод]:
MAPPING LOAD
    FirstValue(%ГодКонтрагентОп) as %ГодКонтрагентОп
    ,1 as фл1
RESIDENT ВрОпл
GROUP BY %КонтрагентОп
ORDER BY %ДатаОп;

[ОплМес]:
MAPPING LOAD
    FirstValue(%МесяцГодКонтрагентОп) as %МесяцГодКонтрагентОп
    ,1 as фл2
RESIDENT ВрОпл
GROUP BY %КонтрагентОп
ORDER BY %ДатаОп;

[ОплКврт]:
MAPPING LOAD
    FirstValue(%КвртКонтрагентОп) as %КвртКонтрагентОп
    ,1 as фл3
RESIDENT ВрОпл
GROUP BY %КонтрагентОп
ORDER BY %ДатаОп;

